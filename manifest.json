{
    "manifest_version": 2,
    "name": "capture-permission-problem",
    "version": "0.1",
    "description": "",
    "permissions" : [
        "tabs",
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "icon_128.png"
    },
    "background": {
        "persistent": true,
        "scripts": [
            "background.js"
        ]
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+X",
                "windows": "Ctrl+Shift+X",
                "mac": "Command+Shift+X",
                "chromeos": "Ctrl+Shift+X",
                "linux": "Ctrl+Shift+X"
            }
        }
    }
}
